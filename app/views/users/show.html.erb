<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h1 data-bind="text: firstName"></h1>
    </div>
    <div class="col-md-6">
      <p>
        <%= link_to "Like Images", "javascript:void(0)" %> |
        <%= link_to "Manage Your Images", "javascript:void(0)" %> |
        <a href="javascript:void(0)" data-bind="click: showEdit">Edit Personal Info</a>
      </p>
    </div>
  </div>
  <hr>
  <div class="row">
    <div class="col-md-9">


    </div>
    <div class="col-md-3">
      <div id="user-edit" class="display-hide">
        <fieldset class="form-group">
          <%= label_tag :first_name %>
          <input data-bind="value: firstName" id="first_name"/>
        </fieldset>
        <fieldset class="form-group">
          <%= label_tag :last_name %>
          <input data-bind="value: lastName" id="last_name"/>
        </fieldset>
        <input type="submit" class="btn btn-default" data-bind="click: hideEdit">
      </div>
    </div>
  </div>

</div>
